{
  "expo": {
    "name": "Testnet",
    "slug": "bitcoin-testnet",
    "version": "0.0.7",
    "orientation": "portrait",
    "scheme": "myapp",
    "newArchEnabled": false,
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#1D1F31",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#1D1F31"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#11131F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ppietras.bitcointestnet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9c5e57e2-6ba9-4c2f-a711-f03222e8c8a4"
      }
    }
  }
}
